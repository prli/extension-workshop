{
    "name": "Spongemock it",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Converts text to the spongebob mocking format. Demo Chrome Extension Workshop.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ]
}