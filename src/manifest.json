{
    "name": "Spongemock it",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Converts text to the spongebob mocking format. Demo Chrome Extension Workshop.",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_icon": {
            "16": "spongemock.png",
            "24": "spongemock.png",
            "32": "spongemock.png"
        },
        "default_title": "SpongeMock It",
        "default_popup": "popup.html"
    }
}